#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>
#define n 10000

int main()
{
    int i;
    int ArrayNum[n];
    double tempoInizio, tempoFine, tempoTotale;
    clock() == 0;
    
    tempoInizio = clock();
    
    srand(time(0));
    for (i = 0; i < n; i++) {
    ArrayNum[i] = 1 + rand()%n;
    }
    
    /*for(i = 0; i < n; i++) {
        printf("%d ", ArrayNum[i]);
    }*/
    
    tempoFine = clock();
    tempoTotale = (double)(tempoFine - tempoInizio) / CLOCKS_PER_SEC;
    printf("\nIl tempo impiegato per la generazione di numeri casuali Ã¨: %f", tempoTotale); 
    
    printf("%ld", CLOCKS_PER_SEC);
}
